<h1><%= @dojo.branch %>'s <%= @student.first_name %> <%= @student.last_name %></h1>
<p>Email: <%= @student.email %></p>
<h2>Cohort</h2>
<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
	</tr>
	<% @cohort.each do |student| %>
		<tr>
			<td><%= student.first_name %></td>
			<td><%= student.last_name %></td>
			<td><%= student.email %></td>
		</tr>
	<% end %>
</table>
<a href="/">Home</a>
<a href="/dojos/<%= @dojo.id %>/students/<%= @student.id %>/edit"></a>