<h1>Listing <%= @dojocount %> Dojos</h1>
<table>
	<tr>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
		<th>Actions</th>
	</tr>
	<% @dojo.each do |dojo| %>
		<tr>
			<td><%= dojo.branch %></td>
			<td><%= dojo.street %></td>
			<td><%= dojo.city %></td>
			<td><%= dojo.state %></td>
			<td>
				<a href="/dojos/<%= dojo.id %>">Show</a> | 
				<a href="/dojos/<%= dojo.id %>/edit">Edit</a> | 
				<a data-method="delete" href="/dojos/<%= dojo.id %>">Delete</a>
			</td>
		</tr>
	<% end %>
</table>
<a href="dojos/new">New Dojo</a>